{
  "name": "learnvue3-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:ci": "vite build",
    "preview": "vite preview",
    "prelint": "yarn format",
    "lint": "eslint --fix \"src/**/*.{ts,vue}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,vue}\"",
    "prettier": "prettier --check \"src/**/*.{ts,vue}\" \"src/**/*.{ts,vue}\"",
    "format": "prettier --write \"src/**/*.{ts,vue}\" \"src/**/*.{ts,vue}\"",
    "check": "yarn lint && yarn prettier",
    "test": "yarn check"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,vue}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.5"
  }
}
